    <Standard_Name rdf:about="http://mmisw.org/ont/cf/areatype/areatype">
        <skos:narrower>
            <Area_Type rdf:about="http://mmisw.org/ont/cf/areatype/c4_plant_functional_types">
                <skos:definition>A plant that utilizes the C4 carbon fixation pathway in which the CO2 is first bound to a compound containing four carbon atoms before photosynthesis reactions take place. All trees are C3 type. Grasses and crops can be C3 or C4.</skos:definition>
            </Area_Type>
        </skos:narrower>
        <skos:narrower>
            <Area_Type rdf:about="http://mmisw.org/ont/cf/areatype/ice_free_land">
                <skos:definition></skos:definition>
